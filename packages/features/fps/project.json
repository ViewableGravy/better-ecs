{
  "name": "fps",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/features/fps/src",
  "projectType": "library",
  "tags": ["type:feature", "domain:fps"],
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "command": "bun x tsc -p tsconfig.lib.json",
        "cwd": "packages/features/fps"
      },
      "inputs": ["default", "^default"],
      "outputs": ["{projectRoot}/dist"]
    },
    "clean": {
      "executor": "nx:run-commands",
      "options": {
        "command": "rm -rf dist tsconfig.lib.tsbuildinfo",
        "cwd": "packages/features/fps"
      }
    },
    "lint": {
      "executor": "nx:run-commands",
      "options": {
        "command": "bun x eslint .",
        "cwd": "packages/features/fps"
      }
    },
    "typecheck": {
      "executor": "nx:run-commands",
      "options": {
        "command": "bun x tsc --noEmit",
        "cwd": "packages/features/fps"
      }
    },
    "test": {
      "executor": "nx:run-commands",
      "options": {
        "command": "bun x vitest run --passWithNoTests",
        "cwd": "packages/features/fps"
      }
    }
  }
}
